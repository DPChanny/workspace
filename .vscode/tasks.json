{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "Run C",
      "dependsOn": [
        "compile-c"
      ],
      "type": "process",
      "command": "powershell",
      "args": [
        "Start-Process",
        "cmd",
        "-ArgumentList",
        "@('/k', '${workspaceFolder}\\.temp\\${relativeFileDirname}\\${fileBasenameNoExtension}.exe')"
      ],
      "problemMatcher": [
        "$gcc"
      ]
    },
    {
      "label": "compile-c",
      "type": "shell",
      "command": ".vscode\\scripts\\compile-c.bat",
      "args": [
        "${file}",
        "${workspaceFolder}\\.temp",
        "${relativeFile}"
      ],
      "options": {
        "env": {
          "DEBUG": "0"
        }
      }
    },
    {
      "label": "compile-debug-c",
      "type": "shell",
      "command": ".vscode\\scripts\\compile-c.bat",
      "args": [
        "${file}",
        "${workspaceFolder}\\.temp",
        "${relativeFile}"
      ],
      "options": {
        "env": {
          "DEBUG": "1"
        }
      }
    },
